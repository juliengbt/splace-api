	"paths": {
		"/equipment": {
			"post": {
				"operationId": "GetEquipments",
				"responses": {
					"200": {
						"description": "OK",
						"content": {
							"application/json": {
								"schema": {
									"items": {
										"$ref": "#/components/schemas/Equipment"
									},
									"type": "array"
								}
							}
						}
					}
				},
				"security": [],
				"parameters": [
					{
						"in": "query",
						"name": "offset",
						"required": false,
						"schema": {
							"format": "double",
							"type": "number"
						}
					}
				],
				"requestBody": {
					"description": "Filters for finding the equipment,\nexample -> Finding volley-ball equipment arround Lannion in an area of 20km^2.\nMust have locker, lighting, shower, handi-access.\nWhere Equipment/Installation name like 'Collège'\nand soil type is beton or synt. Distance is in meters",
					"required": true,
					"content": {
						"application/json": {
							"schema": {
								"$ref": "#/components/schemas/IEquipment"
							},
							"example": {
								"name": "Collège",
								"locker": true,
								"lighting": true,
								"shower": true,
								"sports": [
									{
										"code": "volley"
									}
								],
								"latitude": 48.7333,
								"longitude": -3.4667,
								"gps_area": {
									"max_lat": 48.7922383,
									"max_lon": -3.367779,
									"min_lat": 48.6718575,
									"min_lon": -3.5502904
								},
								"installation": {
									"name": "Collège",
									"disabled_access": true
								},
								"soil_type": [
									{
										"code": "beton"
									},
									{
										"code": "synt"
									}
								],
								"distance": 10000
							}
						}
					}
				}
			}
		},
